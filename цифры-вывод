using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication4
{
    class Program
    {
        static void Main(string[] args)
        {
            string str;
            int k;
            int a;
            Console.WriteLine("введите число");
            str = Console.ReadLine();
            k = int.Parse(str);
            a = 0;
            while (k >= 10)
            {
                a= k % 10;
                k = k / 10;


                Console.WriteLine(a);
                Console.ReadLine();
            }

            
            
        }
    }
}
