using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main()
        {
            int sum = 0;
            int n;
            for (n = 1; n <= 10 n++)
            {
                sum = sum + n * n;
            }
            Console.Write(sum);
            Console.Read ();
        }
    }
}
